<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div><%= f.label :email %><br />
  <%= f.email_field :email %></div>

  <div><%= f.label :password %><br />
  <%= f.password_field :password %></div>

  <div><%= f.label :password_confirmation %><br />
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.label :username_google_calendar %><br />
  <%= f.text_field :username_google_calendar %></div>

  <div><%= f.label :password_google_calendar %><br />
  <%= f.text_field :password_google_calendar %></div>

  <div><%= f.label :username_github %><br />
  <%= f.text_field :username_github %></div>

  <div><%= f.label :password_github %><br />
  <%= f.text_field :password_github %></div>

  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "devise/shared/links" %>
