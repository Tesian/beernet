= form_for(setup_access(@access)) do |f|
  - if @access.errors.any?
    #error_explanation
      %h2
        = pluralize(@access.errors.count, "error")
        prohibited this access from being saved:
      %ul
        - @access.errors.full_messages.each do |msg|
          %li= msg
  .field
    = f.label "Type :"
    %br/
    = f.fields_for :type_access, @access.type_access do |ff|
      = ff.text_field :name
  .field
    = f.label :login
    %br/
    = f.text_field :login
  .field
    = f.label :password
    %br/
    = f.text_field :password
  .field
    = f.label :address
    %br/
    = f.text_field :address
  .actions
    = f.submit
